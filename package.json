{
  "name": "@kewal89/advance-console",
  "version": "0.0.2",
  "description": "Advance Consoles For VS Code & Browsers. Created For Fun. Not For Production.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "yarn build && nodemon src/index.ts",
    "build": "npx tsc -p .",
    "test": "yarn test",
    "pub1": "yarn publish --access public --registry=https://npm.pkg.github.com",
    "pub2": "yarn publish --access public --registry=https://registry.npmjs.org",
    "pub": "yarn pub1 && yarn pub2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kewal89/Advance-Console.git",
    "directory": "build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "console",
    "log",
    "warn",
    "error",
    "info"
  ],
  "author": "Kewal89",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kewal89/Advance-Console/issues"
  },
  "homepage": "https://github.com/Kewal89/Advance-Console#readme",
  "devDependencies": {
    "@types/node": "^18.11.12",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
